# Server Configuration
NODE_ENV=local
PORT=3334
API_URL=http://localhost:3334

# Database
DATABASE_URL=postgresql://luma:luma_password@localhost:5432/luma_db
DB_SSL=false

# Redis
REDIS_URL=redis://localhost:6379

# Stripe Keys
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret
STRIPE_CONNECT_WEBHOOK_SECRET=whsec_your_connect_webhook_secret

# Stripe Price IDs (from your Stripe Dashboard)
STRIPE_PRICE_PRO=price_xxxxxxxxxxxxxx
STRIPE_PRICE_ENTERPRISE=price_yyyyyyyyyyyyyy

# AWS Cognito
AWS_REGION=us-east-1
COGNITO_USER_POOL_ID=us-east-1_xxxxxxxxx
COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxx
COGNITO_CLIENT_SECRET=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx

# Logging
LOG_LEVEL=info

# Cors
CORS_ORIGIN=http://localhost:3000,http://localhost:3001

# Socket.io
SOCKET_IO_PATH=/socket.io

# BullMQ
BULL_REDIS_HOST=localhost
BULL_REDIS_PORT=6379